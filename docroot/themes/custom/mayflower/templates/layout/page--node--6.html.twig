              /* color: #444444; */
              /*color: #0000cc;*/
              color: #14558f;
            }

            .gs-webResult .gs-snippet, .gs-imageResult .gs-snippet, .gs-fileFormatType {
              color: #535353;
            }

            div.gs-visibleUrl, .gs-imageResult div.gs-visibleUrl {
